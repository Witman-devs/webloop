{
  "name": "webloop",
  "private": true,
  "version": "0.0.0",
  "main": "main.js",
  "type": "module",
  "homepage": ".",
  "scripts": {
    "dev": "vite",
    "start": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && npm run electron\"",
    "electron": "electron .",
    "build": "vite build",
    "build-electron": "electron-builder",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d dist"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@faker-js/faker": "^9.9.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^8.9.2",
    "@xyflow/react": "^12.8.2",
    "howler": "^2.2.4",
    "lucide-react": "^0.534.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-joyride": "^2.9.3",
    "react-modal": "^3.16.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^8.2.2",
    "electron": "^37.2.5",
    "electron-builder": "^26.0.12",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "vite": "^7.0.4",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.witman.goldenmare",
    "productName": "WebLoop",
    "files": [
      "dist/**/*",
      "main.js"
    ],
    "win": {
      "target": "nsis",
      "icon": "public/lgoo.ico"
    },
    "directories": {
      "output": "release"
    }
  }
}
